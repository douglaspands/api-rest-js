{
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
        "**/*.js"
    ],
    "exclude": [
        "node_modules",
        "test/**/*.test.js"
    ],
    "ignore-class-method": "methodToIgnore",
    "reporter": [
        "lcov",
        "text-summary",
        "text",
        "html"
    ],
    "cache": true,
    "all": true
}